@model IEnumerable<KeepLearning.Application.Models.Question.QuestionDto>

@{
    ViewData["Title"] = "Resolve";
}

<h1>Resolve</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuestionText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnswerText)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnswerText)
            </td>
        </tr>
}
    </tbody>
</table>
